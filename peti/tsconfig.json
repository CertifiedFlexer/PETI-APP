{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
  "module": "ES2015",
    "esModuleInterop": true,
  "forceConsistentCasingInFileNames": true,
    "jsx": "react",
  "lib": ["es2015", "dom"],
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
, "app/index.tsx"  ]
}
